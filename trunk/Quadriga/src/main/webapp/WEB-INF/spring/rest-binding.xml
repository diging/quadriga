<?xml version="1.0" encoding="UTF-8"?>

<mapping>
	<class name="edu.asu.spring.quadriga.domain.rest.ProjectList">
 		<map-to xml="projectlist" />
 			<field name="projectList" type="edu.asu.spring.quadriga.domain.implementation.Project"   collection="collection">
			<bind-xml name="project" node="element" />
		</field>
 	</class>
	<class name="edu.asu.spring.quadriga.domain.implementation.Project">
 		
 		<field name="name" type="string">
			<bind-xml name="name" node="element" />
		</field>
		<field name="description" type="string">
			<bind-xml name="description" node="element" />
		</field>
		<field name="id" type="string">
			<bind-xml name="id" node="element" />
		</field>
 	</class>
	
 	
</mapping>
