	<Quadriga:term>$dictionaryItems.dictionaryItem.term</Quadriga:term>
	<Quadriga:pos>$dictionaryItems.dictionaryItem.pos</Quadriga:pos>
</Quadriga:dictionaryItem> 	
#end
</Quadriga:dictionaryItemsList>
</Quadriga:QuadrigaReply>