<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Quadriga:QuadrigaReply xmlns:Quadriga="http://www.digitalhps.org/Quadriga" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="$url/resources/restxmlschema/transformationdetails.xsd">
	<Quadriga:nodeList>
		#foreach ($node in $nodeList)
		<Quadriga:node>
			<Quadriga:id>$node.id</Quadriga:id>
			<Quadriga:label>$node.label</Quadriga:label>
			<Quadriga:description>$node.description</Quadriga:description>
			<Quadriga:conceptId>$node.conceptId</Quadriga:conceptId>
		</Quadriga:node> 	
		#end
	</Quadriga:nodeList>
	<Quadriga:linkList>
		#foreach ($link in $linkList)
		<Quadriga:link>
			<Quadriga:subjectId>$link.subject.id</Quadriga:subjectId>
			<Quadriga:objectId>$link.object.label</Quadriga:objectId>
			<Quadriga:label>$link.label</Quadriga:label>
			<Quadriga:statementId>$link.statementId</Quadriga:statementId>
		</Quadriga:link> 	
		#end
	</Quadriga:linkList>
</Quadriga:QuadrigaReply>