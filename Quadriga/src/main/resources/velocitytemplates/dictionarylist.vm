<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Quadriga:QuadrigaReply xmlns:Quadriga="http://www.digitalhps.org/Quadriga" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="$uri/resources/restxmlschema/dictionarylist.xsd">
<Quadriga:dictionaryList>

#foreach ($dictionary in $list)
<Quadriga:dictionary>
	<Quadriga:name>$dictionary.name</Quadriga:name>
	<Quadriga:description>$dictionary.description</Quadriga:description>
	<Quadriga:uri>$uri/rest/dictionaryDetails/$dictionary.id</Quadriga:uri>
</Quadriga:dictionary> 	
#end
</Quadriga:dictionaryList>
</Quadriga:QuadrigaReply>